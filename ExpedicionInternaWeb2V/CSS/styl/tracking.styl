 .glyphicon-track {
  position: relative;
  top: 0px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  font-style: normal;
  font-weight: normal;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  COLOR: #960028;
  font-size: 12px;
  -moz-osx-font-smoothing: grayscale;
}

.glyphicon-track-icon {
  position: relative;
  top: 0px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  font-style: normal;
  font-weight: normal;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  COLOR: #F5F5F5;
  font-size: 19px;
  margin-left: 0px;
  margin-right: 5px;
  -moz-osx-font-smoothing: grayscale;
  animation : logo 1s;
  top: 1px;
  transform: rotate(375deg);
}


.link {     
  color: rgb(52, 20, 255);
  font-family: inherit;
  text-align: center;
  cursor: pointer;
  text-decoration: underline;
}

.modal-header-tracking {
  padding: 23px;
  border-bottom: 39px solid #E2A651;
  min-height: 16.42857143px;
  background-color: rgb(150, 0, 40);
  color: white;
}
.modal-footer-tracking {
  padding: 15px;
  text-align: right;
  border-top: 0px solid #e5e5e5;
}

@keyframes logo
{
  0% {
    opacity: 0;
    transform: scale(.1) rotate(45deg);
  }
  50%{
    transform: scale(1) rotate(445deg);
    font-size: 45px;
  }
  100%{
    transform: scale(1) rotate(375deg);
    opacity: 1;
  }
}