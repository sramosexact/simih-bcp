 .list {
  display: block;
  width: 100%;
  border: 0;
  border-bottom: 1px #e1e1e1 solid;
  padding: 2px 0;
  color: #555555;
  margin-bottom: 0;
}
.list-group {
  display: block;
  position: relative;
}
.list-group .group-title {
  display: block;
  margin-bottom: 2px;
  background-color: #f0f0f0;
  padding: 4px 20px;
  font-family: 'Segoe UI_', 'Open Sans', Verdana, Arial, Helvetica, sans-serif;
  font-size: 11px;
  font-weight: bold;
  color: #333333;
  padding-left: 24px;
}

.list.marked .list-content {
  border-left: 3px #0067b0 solid !important;
}

.list .list-content .list-title {
  font-size: 16px;
  line-height: 20px;
}
.list .list-content .list-title, .listview-outlook .list .list-content .list-subtitle, .list .list-content .list-remark {
  width: 100%;
  display: block;
  color: inherit;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .list-content {
  margin: 2px 0;
  padding: 2px 20px;
  font-family: 'Segoe UI_', 'Open Sans', Verdana, Arial, Helvetica, sans-serif;
  font-size: 16px;
  color: inherit;
  border-left: 3px transparent solid;
}
.list .list-content .list-subtitle {
  font-weight: bold;
  color: #0067cb;
}
.list .list-content .list-subtitle, .list .list-content .list-remark {
  font-size: 11px;
  font-weight: normal;
  line-height: 15px;
  color: #1d1d1d;
}

